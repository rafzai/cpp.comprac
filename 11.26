#include <iostream>
#include <vector>

int main() {
    std::vector<int> arr = {3, -5, 2, -8, 1, -4};

    std::cout << "Non-negative elements: ";
    for (int num : arr) {
        if (num >= 0) {
            std::cout << num << " ";
        }
    }

    std::cout << "\nNegative elements: ";
    for (int num : arr) {
        if (num < 0) {
            std::cout << num << " ";
        }
    }

    return 0;
}
