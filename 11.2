#include <iostream>

int main() {
    int arr[10];

    std::cout << "Enter 10 values:" << std::endl;

    for (int i = 0; i < 10; i++) {
        std::cin >> arr[i];
    }

    std::cout << "Entered values:" << std::endl;
    for (int i = 0; i < 10; i++) {
        std::cout << arr[i] << std::endl;
    }

    return 0;
}
